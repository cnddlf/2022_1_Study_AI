targetCheck].replace(val, median)\n","\n","  return df\n"],"metadata":{"id":"KxulkmFIVst8"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["for idx, row in df_drop_list.iterrows():\n","  df = ConvertAbnormality(df, row['class'], row['hwy'], 'hwy', 'cty', 4.5)\n","  print(df[(df['class']==row['class']) & (df['hwy']==row['hwy'])])"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":211},"id":"rpZvfTVDWcGw","executionInfo":{"status":"error","timestamp":1661245275424,"user_tz":-540,"elapsed":499,"user":{"displayName":"이충일","userId":"14549651031576837673"}},"outputId":"5a6085a9-aea9-4e55-8231-0d71ab39fccd"},"execution_count":null,"outputs":[{"output_type":"error","ename":"TypeError","evalue":"ignored","traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mTypeError\u001b[0m                                 Traceback (most recent call last)","\u001b[0;32m<ipython-input-32-69010fc6fe87>\u001b[0m in \u001b[0;36m<module>\u001b[0;34m\u001b[0m\n\u001b[1;32m      1\u001b[0m \u001b[0;32mfor\u001b[0m \u001b[0midx\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mrow\u001b[0m \u001b[0;32min\u001b[0m \u001b[0mdf_drop_list\u001b[0m\u001b[0;34m.\u001b[0m\u001b[0miterrows\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m:\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\n\u001b[0;32m----> 2\u001b[0;31m   \u001b[0mdf\u001b[0m \u001b[0;34m=\u001b[0m \u001b[0mConvertAbnormality\u001b[0m\u001b[0;34m(\u001b[0m\u001b[0mdf\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mrow\u001b[0m\u001b[0;34m[\u001b[0m\u001b[0;34m'class'\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0mrow\u001b[0m\u001b[0;34m[\u001b[0m\u001b[0;34m'hwy'\u001b[0m\u001b[0;34m]\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m'hwy'\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;34m'cty'\u001b[0m\u001b[0;34m,\u001b[0m \u001b[0;36m4.5\u001b[0m\u001b[0;34m)\u001b[0m\u001b[0;34m\u001b[0m\u001b[0;34m\u001b[0m\u001b[0m\